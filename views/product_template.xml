<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Inherit Form view -->
        <record model="ir.ui.view" id="export_complements_product_template_form">
            <field name="name">export_complements.product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="stock.view_template_property_form"/>
            <field name="arch" type="xml">
                <group name="sale_condition" position="after">
                    <group name="export_complements" string="Export Complements">
                        <field name="tariff_fraction"/>
                        <field name="export_oum"/>
                    </group>
                </group>
            </field>
        </record>
    </data>
</odoo>
